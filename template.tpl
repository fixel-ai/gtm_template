    assertApi('gtmOnFailure').wasNotCalled();
setup: |
  const log = require('logToConsole');
  const setInWindow = require('setInWindow');

  const mockData = {
    fxl_id: '12345'
  };
  const scriptUrl = 'https://script.fixel.ai/script/Fixel.min.js?src=gtm&fxl_code='+mockData.fxl_id;

  let success,failure;

  mock('injectScript', (url, onsuccess, onfailure) => {
    success = onsuccess;
    failure = onfailure;
    onsuccess();
  });


